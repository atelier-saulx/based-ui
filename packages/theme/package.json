{
  "name": "@based/theme",
  "version": "0.1.7",
  "repository": "https://github.com/atelier-saulx/based-ui",
  "description": "A front-end library of themes for Based",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "dependencies": {
    "@saulx/hash": "^1.1.0",
