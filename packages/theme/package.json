  "devDependencies": {
    "@based/tsconfig": "*",
    "@saulx/eslint-config": "^1.1.0",
    "@saulx/prettier-config": "^1.0.0",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
